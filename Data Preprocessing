      "source": [
        "from google.colab import files\n",
        "uploaded = files.upload()"
      ],
      "metadata": {
        "colab": {
          "base_uri": "https://localhost:8080/",
          "height": 73
        },
        "id": "P-_LSTWwo_Zk",
        "outputId": "983db4b8-f3b6-4aa2-d9db-4dde2cf1fd4e"
